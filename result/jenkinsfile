pipeline {
    agent any
    tools{
      nodejs 'Nodejs 8.9.0'
}
stages{
    stage (build){
    
    steps{

      echo 'Compiling result app'
      dir ('result'){
           sh 'npm install'
        }
        
    }
 }


    stage (Test){
  
    steps{

      echo 'Compiling result app'
      dir('result'){
                  sh 'npm install'
                  sh 'npm test'
                }
        
    }
}
}
}